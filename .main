// PARA CHEQUEAR LAS PALABRAS CON CONCEPTOS //
boolean checkWord(String[] search , String s)
{ boolean check = false;
  for (int i = 0 ; i<search.length ; i++)
  {
    if (s.equalsIgnoreCase(search[i])) check = true;
  }
  return check;
}
